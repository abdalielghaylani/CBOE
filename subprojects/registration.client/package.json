  "license": "ISC",
  "description": "PerkinElmer ChemBioOffice Enterprise Registration Client",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "cover": "istanbul report text text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --colors --progress --display-error-details --display-cached --content-base src",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-css": "stylelint './src/**/*.css'",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production webpack-dev-server --inline --colors --progress --display-error-details --display-cached --content-base src",
    "test": "karma start ./karma/karma.conf.js",
    "test:headless": "karma start ./karma/karma.conf.js --autoWatch=true --singleRun=false",
    "test:single": "karma start ./karma/karma.conf.js --singleRun=true"
  },
  "repository": {},
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/atmosphere": "^2.1.32",
