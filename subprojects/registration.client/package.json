    "@types/node": "^6.0.52",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enhanced-resolve": "^3.0.1",
    "eslint": "^3.12.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "^0.5.1",
