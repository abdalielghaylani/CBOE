<script language="javascript">

function goAdminTables(manage_type, table_name,sql_string){

		actiontemp = action_form_path
		MainWindow.document.cows_input_form.sql_string.value = sql_string
		MainWindow.document.cows_input_form.action= actiontemp + "?dbname=<%=dbkey%>&formgroup=" + manage_type + "_form_group&dataaction=get_sql_string&sql_source=sql_string&base_table_name=" + table_name
		MainWindow.document.cows_input_form.submit()
}

function goAdminTables2(table_name,sql_string){
		actiontemp = action_form_path
		MainWindow.document.cows_input_form.sql_string.value = sql_string
		MainWindow.document.cows_input_form.action= actiontemp + "?dbname=<%=dbkey%>&formgroup=manage_tables_form_group&dataaction=get_sql_string&sql_source=sql_string&base_table_name=" + table_name
		MainWindow.document.cows_input_form.submit()
}

</script>

<%
privTable = Application("Priv_Table_Name")
dba_users_sql_string = getUsersListSQL("", dbkey, formgroup, privTable)
roles_sql_string = getRolesListSQl(dbkey, formgroup, privTable)
people_sql_string = "Select person_id from people"
sites_sql_string = "Select site_id from sites"
%>
</head>

<body>

<!--#INCLUDE FILE = "../../source/app_vbs.asp"-->
<!--#INCLUDE VIRTUAL = "/cfserverasp/source/header_vbs.asp"-->

<input type="hidden" name="sql_string" value>

<table cellspacing="0" cellpadding="0">

<%if Session("Add_USERS_Table" & dbkey) OR Session("Edit_USERS_Table"& dbkey) OR Session("Delete_USERS_Table"& dbkey) then%>
	<tr>
		<td><a href="javascript:goAdminTables('manage_users','dba_users',&quot;<%=dba_users_sql_string%>&quot;)"><img SRC="/<%=Application("AppKey")%>/graphics/usr_mgr_btn.gif" alt="add/edit users" BORDER="0"></a>
		</td>
	</tr>
<%end if%>

<%if UCase(Application("AppKey")) = "MIX_REG" and (Session("Add_Roles_Table" & dbkey) OR Session("Edit_Roles_Table"& dbkey) OR Session("Delete_Roles_Table"& dbkey)) then%>
	<tr>	
		<td><a href="javascript:goAdminTables('manage_roles','security_roles',&quot;<%=roles_sql_string%>&quot;)"><img SRC="/<%=Application("AppKey")%>/graphics/manage_roles_btn.gif" BORDER="0" alt="add/edit roles"></a>
		</td>
	</tr>
<%else%>
	<%if Session("Add_USERS_Table" & dbkey) OR Session("Edit_USERS_Table"& dbkey) OR Session("Delete_USERS_Table"& dbkey) then%>
		<% if UCase(Application("AppKey")) = "BIOSAR_BROWSER" AND (Session("Add_Roles_Table" & dbkey) OR Session("Edit_Roles_Table"& dbkey) OR Session("Delete_Roles_Table"& dbkey)) then%>
		<tr>	
			<td><a href="javascript:goAdminTables('manage_roles','security_roles',&quot;<%=roles_sql_string%>&quot;)"><img SRC="/<%=Application("AppKey")%>/graphics/manage_roles_btn.gif" BORDER="0" alt="add/edit roles"></a>
			</td>
		</tr>
		<%else%>
			<%if NOT(UCase(Application("AppKey")) = "BIOSAR_BROWSER") then%>
			
			<tr>	
				<td><a href="javascript:goAdminTables('manage_roles','security_roles',&quot;<%=roles_sql_string%>&quot;)"><img SRC="/<%=Application("AppKey")%>/graphics/manage_roles_btn.gif" BORDER="0" alt="add/edit roles"></a>
				</td>
			</tr>
			<%end if%>
		<%end if%>
	<%end if%>
<%end if%>


<%if CBool(Application("ShowSecTabinUsrMgr")) = True then %>
	<%if (Session("Edit_People_Table" & dbkey) OR Session("ADD_People_Table" & dbkey) OR Session("DElete_People_Table" & dbkey)) then%>
		<tr>
			<td><a href="javascript:goAdminTables2('people','<%=people_sql_string%>')"><img SRC="/<%=Application("AppKey")%>/graphics/manage_people_btn.gif" BORDER="0" alt="edit/add to people table"></a>
			</td>
		</tr>
	<%end if%>
   
	<%if Session("Edit_Sites_Table" & dbkey) = true  AND CBool(Application("Sites_Used")) = True then%>
		<tr>
			<td><a href="javascript:goAdminTables2('sites','<%=sites_sql_string%>')"><img SRC="/<%=Application("AppKey")%>/graphics/manage_sites_btn.gif" BORDER="0" alt="edit/add to sites table"></a>
			</td>
		</tr>
	<%end if%>
<%end if%>
   
</table>

<!--#INCLUDE VIRTUAL = "/cfserverasp/source/input_form_footer_vbs.asp"-->

</body>
</html>
