<%@ EnableSessionState=False Language=VBScript%>
<!--#INCLUDE VIRTUAL = "/cfserverasp/source/ado.inc"-->
<!--#INCLUDE VIRTUAL = "/cfserverasp/source/server_const_vbs.asp"-->
<!--#INCLUDE VIRTUAL = "/cheminv/api/apiUtils.asp"-->
<Script RUNAT="Server" Language="VbScript">
Dim LocationID
Dim ParentID
Dim Conn
Dim Cmd

Dim strError
Dim bWriteError
Dim PrintDebug

bDebugPrint = False
bWriteError = False
strError = "Error:UpdateContainerQtyRemaining<BR>"

BumpUp = Request("BumpUp")


'if Len(Request.QueryString) = 0 AND Len(Request.Form)= 0 then
'	Response.Redirect "/cheminv/help/admin/api/UpdateContainerQtyRemaining.htm"
'	Response.end
'End if


' Check for required parameters
if IsEmpty(BumpUp) then
	BumpUp=0
End if

' Respond with Error
if bWriteError then
	Response.Write strError
	Response.end
End if



' Set up and ADO command
Call GetInvCommand(Application("CHEMINV_USERNAME") & ".DFCI_CREATEPARLEVELREQUESTS", adCmdStoredProc)

' Code generated by QueryProcParams.asp helper page
'Cmd.Parameters.Append Cmd.CreateParameter("RETURN_VALUE",adNumeric, adParamReturnValue, 0, NULL)
'Cmd.Parameters("RETURN_VALUE").Precision = 9
Cmd.Parameters.Append Cmd.CreateParameter("RETURN_VALUE",advarchar, adParamReturnValue, 2000, NULL)
Cmd.Parameters.Append Cmd.CreateParameter("LocationId", adNumeric, adParamInput, 9, Null)

Cmd.Parameters.Append Cmd.CreateParameter("bumpup",adDouble, adParamInput, 0, BumpUp)


if bDebugPrint then
	For each p in Cmd.Parameters
		Response.Write p.name & " = " & p.value & "<BR>"
	Next	
Else
	Call ExecuteCmd(Application("CHEMINV_USERNAME") & ".DFCI_CREATEPARLEVELREQUESTS")
End if

Response.Write Cmd.Parameters("RETURN_VALUE")

'Clean up
Conn.Close
Set Conn = Nothing
Set Cmd = Nothing
</SCRIPT>
