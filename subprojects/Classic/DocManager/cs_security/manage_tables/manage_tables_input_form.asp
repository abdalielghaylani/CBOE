<%@ LANGUAGE=VBScript%>
<%Response.expires=0
'Copyright 1998-2001 CambridgeSoft Corporation All Rights Reserved
'DO NOT EDIT THIS FILE
if not dbkey <> "" then dbkey=request("dbname")
formgroup= request("formgroup")

if Not Session("UserValidated" & dbkey) = 1 then  response.redirect "/" & Application("Appkey") & "/logged_out.asp"%>
<%if CBool(Application("ShowSecTabinUsrMgr")) = True then 
	response.redirect "/" & Application("AppKey") & "/cs_security/manage_security/manage_security_input_form.asp?nav_override=true&formgroup=manage_users_form_group&formmode=manage_users&dbname=" & dbkey & "&timer=" & Timer
else
	response.redirect  "/" & Application("AppKey") & "/admin/manage_reg_tables/manage_reg_tables_input_form.asp?nav_override=true&formgroup=manage_reg_tables_form_group&formmode=manage_reg_tables&dbname=" & dbkey & "&timer=" & Timer
end if%>

<html>
<head>


<!--#INCLUDE VIRTUAL = "/cfserverasp/source/cows_func_js.asp"-->
<!--#INCLUDE FILE = "../../source/secure_nav.asp"-->

<script language="javascript">

function goAdminTables(table_name,sql_string){
		actiontemp = action_form_path
		MainWindow.document.cows_input_form.sql_string.value = sql_string
		MainWindow.document.cows_input_form.action= actiontemp + "?dbname=<%=dbname%>&formgroup=manage_tables_form_group&dataaction=get_sql_string&sql_source=sql_string&base_table_name=" + table_name
		MainWindow.document.cows_input_form.submit()
}

</script>

<%
people_sql_string = "Select person_id from people"
sites_sql_string = "Select site_id from sites"


%>

</head>
<body <%=Application("BODY_BACKGROUND")%>>



<!--#INCLUDE FILE = "../../source/app_vbs.asp"-->
<!--#INCLUDE VIRTUAL = "/cfserverasp/source/header_vbs.asp"-->

<input type="hidden" name="sql_string" value>

<table width="373">

	<%if Session("Edit_People_Table" & dbkey) then%>

  <tr>
    <td align="left" width="220"><a href="javascript:goAdminTables('people','<%=people_sql_string%>')"><img SRC="/<%=Application("AppKey")%>/graphics/manage_people_btn.gif" BORDER="0" alt="edit/add to people table"></a></td>
  </tr>
   <%end if%>
   
   <%if Session("Edit_Sites_Table" & dbkey) = true  AND Application("Sites_Used") = True then%>

  <tr>
    <td align="left" width="220"><a href="javascript:goAdminTables('sites','<%=sites_sql_string%>')"><img SRC="/<%=Application("AppKey")%>/graphics/manage_sites_btn.gif" BORDER="0" alt="edit/add to sites table"></a></td>
  </tr>
   <%end if%>

</table>




<!--#INCLUDE VIRTUAL = "/cfserverasp/source/input_form_footer_vbs.asp"-->

</body>
</html>
