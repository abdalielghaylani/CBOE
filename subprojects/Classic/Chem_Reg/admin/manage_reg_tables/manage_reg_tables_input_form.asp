<%@ LANGUAGE=VBScript %>
<%	Response.expires=0
Response.Buffer = true
'Copyright 1999-2003 CambridgeSoft Corporation. All rights reserved
'DO NOT EDIT THIS FILE

dbkey="reg"
if Not Session("UserValidated" & dbkey) = 1 then  response.redirect "/" & Application("Appkey") & "/logged_out.asp"
OverrideManageHits = true
OverrideManageQueries=true%>

<html>
<head>


<!--#INCLUDE VIRTUAL = "/cfserverasp/source/cows_func_js.asp"-->
<!--#INCLUDE FILE = "../../source/secure_nav.asp"-->

<script language = "javascript">

function goAdminTables(table_name){
		actiontemp = action_form_path
		MainWindow.document.cows_input_form.action= actiontemp + "?dbname=reg&formgroup=manage_reg_tables_form_group&dataaction2=MANAGE_REG_TABLES_LIST&base_table_name=" + table_name
		MainWindow.document.cows_input_form.submit()
}


</script>

<% Session("table_name")=""
bSITES = (bEDIT_SITES_TABLE= True OR bADD_SITES_TABLE= True OR bDELETE_SITES_TABLE= True) AND CBool(Application("SITES_Used")) = true
bPROJECTS =(bEDIT_PROJECTS_TABLE= True OR bADD_PROJECTS_TABLE= True OR bDELETE_PROJECTS_TABLE= True) AND CBool(Application("PROJECTS_Used")) = true
bBATCH_PROJECTS =(bEDIT_BATCH_PROJECTS_TABLE= True OR bADD_BATCH_PROJECTS_TABLE= True OR bDELETE_BATCH_PROJECTS_TABLE= True) AND CBool(Application("BATCH_PROJECTS_Used")) = true
bSALTS =(bEDIT_SALT_TABLE= True OR bADD_SALT_TABLE= True OR bDELETE_SALT_TABLE= True) AND CBool(Application("SALTS_Used")) = true
bSOLVATES =(bEDIT_SOLVATES_TABLE= True OR bADD_SOLVATES_TABLE= True OR bDELETE_SOLVATES_TABLE= True) AND CBool(Application("SOLVATES_Used")) = true
bPEOPLE = (bEDIT_PEOPLE_TABLE= True OR bADD_PEOPLE_TABLE= True OR bDELETE_PEOPLE_TABLE= True) AND CBool(Application("SHOW_SEC_TBLES_IN_USR_MGR")) = false
bSEQUENCES = bEDIT_SEQUENCES_TABLE= True OR bADD_SEQUENCES_TABLE= True OR bDELETE_SEQUENCES_TABLE= True		
bNOTEBOOKS=(bEDIT_NOTEBOOKS_TABLE= True OR bADD_NOTEBOOKS_TABLE= True OR bDELETE_NOTEBOOKS_TABLE= True) AND CBool(Application("NOTEBOOK_USED")) = true AND CBool(Application("NOTEBOOK_LOOKUP"))= True
bUTILIZATIONS=(bEDIT_UTILIZATIONS_TABLE= True OR bADD_UTILIZATIONS_TABLE= True OR bDELETE_UTILIZATIONS_TABLE= True) AND CBool(Application("UTILIZATION_PERMISSIONS_USED")) = true
bCOMPOUND_TYPES = (bEDIT_COMPOUND_TYPE_TABLE= True OR bADD_COMPOUND_TYPE_TABLE= True OR bDELETE_COMPOUND_TYPE_TABLE= True) AND CBool(Application("COMPOUND_TYPES_USED")) = true
%>

</head>
<body>

<!--#INCLUDE FILE = "../../source/app_vbs.asp"-->
<!--#INCLUDE VIRTUAL = "/cfserverasp/source/header_vbs.asp"-->

<table width="373">

<% if bPROJECTS = True then%>
	<tr>
		<%if CBOOL(Application("PROJECTS_NAMED_OWNER")) = True  then%>
				<td align="left" width="220"><a href="javascript:goAdminTables('projects')"><img
				SRC="/<%=Application("AppKey")%>/graphics/owners_table_btn.gif" alt="edit/add to owners table" BORDER="0"></a></td>
		<%else%>
				<td align="left" width="220"><a href="javascript:goAdminTables('projects')"><img
				SRC="/<%=Application("AppKey")%>/graphics/projects_table_btn.gif" alt="edit/add to projects table" BORDER="0"></a></td>
		<%end if%>
     </tr>
<%end if%>

	<%if bNOTEBOOKS = true then%>
  <tr>	
    <td align="left" width="220"><a
    href="javascript:goAdminTables('notebooks')"><img
    SRC="/<%=Application("AppKey")%>/graphics/notebooks_table_btn.gif" BORDER="0" alt="edit/add to notebooks table"></a></td>
  </tr>
    <%end if%>
  <%if bSEQUENCES = true  then%>

  <tr>
    <td align="left" width="220"><a
    href="javascript:goAdminTables('sequence')"><img
    SRC="/<%=Application("AppKey")%>/graphics/sequences_table_btn.gif" BORDER="0" alt="edit/add to sequence table"></a></td>
  </tr>
   <%end if%>
   
    <%if bCOMPOUND_TYPES = true then%>

  <tr>
    <td align="left" width="220"><a
    href="javascript:goAdminTables('compound_type')"><img
    SRC="/<%=Application("AppKey")%>/graphics/manage_structure_comments_table_btn.gif" BORDER="0" alt="edit/add to stucture comments table"></a></td>
  </tr>
   <%end if%>
	<%if bPEOPLE = true then%>


		<tr>
		  <td align="left" width="220"><a
		  href="javascript:goAdminTables('people')"><img
		  SRC="/<%=Application("AppKey")%>/graphics/manage_people_btn.gif" BORDER="0" alt="edit/add to people table"></a></td>
		</tr>
	 <%end if%>
	<%if bSITES = true then%>	
		 
		<tr>
		  <td align="left" width="220"><a
		  href="javascript:goAdminTables('sites')"><img
		  SRC="/<%=Application("AppKey")%>/graphics/manage_sites_btn.gif" BORDER="0" alt="edit/add to sites table"></a></td>
		</tr>
		
   <%end if%>
	<%if bSALTS = true then%>
		<tr>
			<td align="left" width="220"><a
			href="javascript:goAdminTables('salts')"><img
			SRC="/<%=Application("AppKey")%>/graphics/salts_table_btn.gif" BORDER="0" alt="edit/add to salts table"></a></td>
		</tr>
   <%end if%>
	<%if bSOLVATES = true then%>
		<tr>
			<td align="left" width="220"><a
			href="javascript:goAdminTables('solvates')"><img
			SRC="/<%=Application("AppKey")%>/graphics/solvates_table_btn.gif" BORDER="0" alt="edit/add to solvates table"></a></td>
		</tr>
   <%end if%>
   
 
   
	<%if bUTILIZATIONS = true then%>
		<tr>
			<td align="left" width="220"><a
		    href="javascript:goAdminTables('utilizations')"><img
		    SRC="/<%=Application("AppKey")%>/graphics/utilizations_btn.gif" BORDER="0" alt="edit/add to utilization permissions"></a></td>
		</tr>
   <%end if%>
	<%if bBATCH_PROJECTS = true then%>

		<tr>
			<td align="left" width="220"><a
		    href="javascript:goAdminTables('batch_projects')">
		    <%if CBOOL(Application("PROJECTS_NAMED_OWNER")) = True  then%>
		    <img
		    SRC="/<%=Application("AppKey")%>/graphics/projects_table_btn.gif" BORDER="0" alt="edit/add to  batch projects"></a></td>
			<%else%>
					    <img
		    SRC="/<%=Application("AppKey")%>/graphics/batch_projects_btn.gif" BORDER="0" alt="edit/add to batch projects"></a></td>

			<%end if%>
		</tr>
   <%end if%>
   

</table>




<!--#INCLUDE VIRTUAL = "/cfserverasp/source/input_form_footer_vbs.asp"-->

</body>
</html>
