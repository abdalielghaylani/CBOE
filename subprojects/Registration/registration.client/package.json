{
  "name": "registration.client",
  "version": "1.0.0",
  "license": "ISC",
  "description": "PerkinElmer ChemBioOffice Enterprise Registration Client",
  "keywords": [
    "angular2",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": "6"
  },
  "scripts": {
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "prebuild": "npm run clean && npm run fix-memory-limit",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "cover": "istanbul report --include=**/chrome/coverage-final.json text text-summary",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development PORT=8080 webpack-dev-server -d --inline --progress --no-info",
    "dev": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "rimraf coverage/ & npm run lint",
    "test": "karma start --singleRun true --browsers=Chrome",
    "test:ci": "karma start --singleRun true --browsers=ChromeHeadless",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false"
  },
  "repository": {},
