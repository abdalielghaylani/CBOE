  <!-- Restrict to compounds whose formula is C6H4Cl2 -->
  <searchCriteriaItem id="6"  
		 fieldid="16" 
		 modifier="" 
		 tableid="3">
		<formulaCriteria>C6H4Cl2</formulaCriteria>
	</searchCriteriaItem>

  <!-- Restrict to compounds that comply 10 <= molweight <= 1000 -->
	<searchCriteriaItem id="7"  
		 fieldid="16" 
		 modifier="" 
		 tableid="3">
		<molweightCriteria min="10" max="1000" />
	</searchCriteriaItem>

  <!-- Restrict to compounds whose substance_name equals "1,2-dichlorobenZene" -->
  <searchCriteriaItem id="8"  
		 fieldid="15" 
		 modifier="" 
		 tableid="3">
		<textCriteria trim="left" 	
			caseSensitive="NO" 
			operator="EQUAL" 
			fullWordSearch="NO"> 1,2-dichlorobenZene</textCriteria>
	</searchCriteriaItem>

  <!-- Restrict to compounds whose CAS is unassigned (Database value NULL) -->
  <searchCriteriaItem id="9"  
		 fieldid="17" 
		 modifier="" 
		 tableid="3">
		<numericalCriteria trim="both" 	
			caseSensitive="NO" 
			operator="EQUAL" >NULL</numericalCriteria>
	</searchCriteriaItem>

  <!-- Restrict to compounds whose compound_id is any of 2020, 2021, 2022 or 2023 -->
  <searchCriteriaItem id="10"  
		 fieldid="14" 
		 modifier="" 
		 tableid="3">
		<numericalCriteria trim="left" operator="IN" >2020, 2021, 2022, 2023</numericalCriteria>
	</searchCriteriaItem>
  
  <searchCriteriaID>1</searchCriteriaID>
