<searchCriteria xmlns="COE.SearchCriteria">
  <!--  This search Criteria as a whole is too restrictive, so no records will match it. 
        Its purpose is to exemplify the syntax of searchCriterias.
        Criterias should be tested seperatedly or in small groups in order to achieve visible results
  -->
  <!-- Restrict to compounds whose compound_id >= 100. Note this is an example of a numerical criteria of input of type number (100) and database field of type number (as stated in dataview field 14)-->
  <searchCriteriaItem id="1"  
					 fieldid="14" 
					 modifier="" 
					 tableid="3">
		<numericalCriteria operator="GTE" trim="BOTH">100</numericalCriteria>
	</searchCriteriaItem>

  <!-- Restrict to compounds whose compoudn_id != 283-->
	<searchCriteriaItem id="2"  
					 fieldid="14" 
					 modifier="" 
					 tableid="3">
		<numericalCriteria operator="NOTEQUAL" trim="NONE">283</numericalCriteria>
	</searchCriteriaItem>
  
	<!-- Restrict to compounds whose timestamp < 1/1/2006 -->
  <searchCriteriaItem id="3"  
					 fieldid="28" 
					 modifier="" 
					 tableid="3">
		<dateCriteria operator="LT">1/1/2006</dateCriteria>
	</searchCriteriaItem>

  <!-- Restrict to compounds whose substance_name contains the word "benzene" -->
	<searchCriteriaItem id="4"  
					 fieldid="15" 
					 modifier="" 
					 tableid="3">
		<textCriteria operator="LIKE">*benzene*</textCriteria>
	</searchCriteriaItem>
	
