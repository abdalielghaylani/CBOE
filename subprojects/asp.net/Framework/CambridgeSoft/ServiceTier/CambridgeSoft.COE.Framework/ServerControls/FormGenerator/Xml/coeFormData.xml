<?xml version="1.0"?>
<COEForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="COE.COEForm">
  <formDisplay>
    <height>500px</height>
    <width>500px</width>
    <style>Border-Color:red;Border-Style:solid;Border-Width:1px</style>
  </formDisplay>
  <formElements>
    <formElement>
      <defaultValue>Compound Id: </defaultValue>
      <configInfo>&lt;fieldConfig&gt;
                &lt;Style&gt;border-color:blue;&lt;/Style&gt;
                &lt;Height&gt;50&lt;/Height&gt;
                &lt;Width&gt;150&lt;/Width&gt;
                &lt;ID&gt;label1&lt;/ID&gt;
             &lt;/fieldConfig&gt;</configInfo>
      <displayInfo>
        <height>50px</height>
        <width>150px</width>
        <top>10px</top>
        <left>10px</left>
        <type>CambridgeSoft.COE.Framework.Controls.FormGenerator.COELabel</type>
      </displayInfo>
    </formElement>
    <formElement>
      <defaultValue>1515</defaultValue>
      <bindingExpression>CompoundId</bindingExpression>
      <configInfo>&lt;fieldConfig&gt;
                &lt;Style&gt;border-color:red;&lt;/Style&gt;
                &lt;MaxLength&gt;30&lt;/MaxLength&gt;
                &lt;Height&gt;50&lt;/Height&gt;
                &lt;Width&gt;150&lt;/Width&gt;
                &lt;ID&gt;CompoundId&lt;/ID&gt;
             &lt;/fieldConfig&gt;</configInfo>
      <displayInfo>
        <height>50px</height>
        <width>250px</width>
        <top>10px</top>
        <left>170px</left>
        <type>CambridgeSoft.COE.Framework.Controls.FormGenerator.COETextBox</type>
      </displayInfo>
    </formElement>
    <formElement>
      <defaultValue>Substance Name: </defaultValue>
      <configInfo>&lt;fieldConfig&gt;
                &lt;Style&gt;border-color:blue;&lt;/Style&gt;
                &lt;Height&gt;50&lt;/Height&gt;
                &lt;Width&gt;150&lt;/Width&gt;
                &lt;ID&gt;label2&lt;/ID&gt;
             &lt;/fieldConfig&gt;</configInfo>
      <displayInfo>
        <height>50px</height>
        <width>150px</width>
        <top>110px</top>
        <left>10px</left>
        <type>CambridgeSoft.COE.Framework.Controls.FormGenerator.COELabel</type>
      </displayInfo>
    </formElement>
    <formElement>
      <defaultValue>dichloro-benzene</defaultValue>
      <bindingExpression>SubstanceName</bindingExpression>
      <configInfo>&lt;fieldConfig&gt;
                &lt;Style&gt;border-color:olive;&lt;/Style&gt;
                &lt;MaxLength&gt;30&lt;/MaxLength&gt;
                &lt;Height&gt;50&lt;/Height&gt;
                &lt;Width&gt;150&lt;/Width&gt;
                &lt;ID&gt;SubstanceName&lt;/ID&gt;
             &lt;/fieldConfig&gt;</configInfo>
      <displayInfo>
        <height>50px</height>
        <width>250px</width>
        <top>110px</top>
        <left>170px</left>
        <type>CambridgeSoft.COE.Framework.Controls.FormGenerator.COETextBox</type>
      </displayInfo>
    </formElement>
  </formElements>
</COEForm>