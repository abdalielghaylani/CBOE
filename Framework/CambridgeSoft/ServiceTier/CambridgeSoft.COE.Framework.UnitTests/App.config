    </sources>
  </enterpriseLibrary.ConfigurationSource>
  <coeLoggingConfiguration logEntryIdentifier="COEFrameWorkUnitTests" enabled="false" categories="All" priority="10" />
  <appSettings>
    <!-- Settings for specifying how a user will be authenticated when using COE Framework Services
    For custom authentication set valuse to: Csla 
    For windows authentication set it to: Windows
    -->
    <add key="CslaAuthentication" value="Csla" />
    <add key="AppName" value="SAMPLE"/>
    <add key="LogonUserName" value="cssadmin"/>
    <add key="LogonPassword" value="cssadmin"/>
    <add key="TestDBName" value="D3DATA"/>
    <add key="TestDBPassword" value="ORACLE"/>
    <add key="underprivilegeduser" value="cssuser"/>

    <!--Settings for specifying how this application will call COE Framework Services-->

    <!--LOCAL:if the framework services are on this machine do not uncomment anything
    -->
    <!--REMOTE if framework services will be accessed via remoting calls uncomment the following lines and enter replace //localhost with the name of the server. For 
    for developement use http://localhost:4285/CambridgeSoft.COE.Framework.RemotingHost/RemotingPortal.rem  change the portnumber if you change the properties to dynamic port numbers
    <add key="CslaDataPortalProxy" value="Csla.DataPortalClient.RemotingProxy,  Csla, Version=2.1.1.0, Culture=neutral, PublicKeyToken=93be5fdc093e4c30"/>
    <add key="CslaDataPortalUrl" value="http://localhost/COERemotingHost/RemotingPortal.rem"/>
    <add key="CslaEncryptRemoting" value="false"/>-->

    <!--
    <add key="CslaDataPortalProxy" value="Csla.DataPortalClient.WebServicesProxy,  Csla, Version=2.1.1.0, Culture=neutral, PublicKeyToken=93be5fdc093e4c30"/>
    <add key="CslaDataPortalUrl" value="http://localhost/WebServiceHost/WebServicePortal.asmx"/>
    -->

  </appSettings>


  <!--this should be commented out only in developement environments
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <linkedConfiguration href="file://C:\Documents and Settings\All Users\Application Data\ChemOfficeEnterprise11\COESharedAssemblyConfig.xml"/>
    </assemblyBinding>
  -->

  <connectionStrings>
    <add name="coedbConn" providerName="Oracle.DataAccess.Client" connectionString="user id=COEDB;proxy user id=COEUSER;proxy password=ORACLE;Data Source=dtp283/ORCL;"/>
    <add name="regdbConn" providerName="Oracle.DataAccess.Client" connectionString="user id=REGDB;proxy user id=COEUSER;proxy password=ORACLE;Data Source=dtp283/orcl;"/>
  </connectionStrings>

  <applicationSettings>
    <CambridgeSoft.COE.Framework.UnitTests.Properties.Settings>
      <setting name="CambridgeSoft_COE_Framework_UnitTests_FrameworkProxy_COEFrameworkServices"
        serializeAs="String">
        <value>http://localhost/COEFrameworkServices/COEFrameworkServices.asmx</value>
      </setting>
    </CambridgeSoft.COE.Framework.UnitTests.Properties.Settings>
